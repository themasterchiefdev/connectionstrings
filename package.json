{
	"name": "Build-Connection-Strings",
	"version": "0.1.0",
	"private": false,
	"dependencies": {
		"@githubprimer/octicons-react": "^8.1.0",
		"bootstrap": "^4.1.3",
		"clipboard": "^2.0.1",
		"font-awesome": "^4.7.0",
		"gh-pages": "^2.0.0",
		"react": "^16.5.1",
		"react-dom": "^16.5.1",
		"react-scripts-ts": "2.17.0"
	},
	"scripts": {
		"predeploy": "npm run build",
		"deploy": "gh-pages -d build",
		"start": "react-scripts-ts start",
		"build": "yarn clean:js && yarn clean:docsfolder && yarn react-scripts-ts build && yarn copyfiles",
		"test": "react-scripts-ts test --env=jsdom",
		"eject": "react-scripts-ts eject",
		"copyfiles": "cpx \"build/**/*.*\" docs/\"",
		"clean:js": "del-cli build/**/*.*",
		"clean:docsfolder": "del-cli docs/**/*.*"
	},
	"devDependencies": {
		"@types/clipboard": "^2.0.1",
		"@types/jest": "^23.3.2",
		"@types/node": "^10.9.4",
		"@types/react": "^16.4.9",
		"@types/react-dom": "^16.0.7",
		"clean-webpack-plugin": "^0.1.19",
		"del-cli": "^1.1.0",
		"typescript": "^3.0.3"
	},
	"homepage": "https://yvrkarthik.github.io/connectionstrings/",
	"license": "MIT"
}
